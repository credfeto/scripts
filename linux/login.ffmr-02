#! /bin/sh

flatpak update -y
flatpak uninstall --unused -y

flatpak run --branch=stable --arch=x86_64 --command=brave --file-forwarding com.brave.Browser --enable-feature=UseOzonePlatform --ozone-platform=wayland @@u %U @@ &

flatpak run com.google.Chrome --enable-feature=UseOzonePlatform --ozone-platform=wayland &

flatpak run org.mozilla.firefox &

flatpak run com.discordapp.Discord &

flatpak run org.telegram.desktop &

flatpak run us.zoom.Zoom &

flatpak run org.kde.marknote &
